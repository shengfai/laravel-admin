        "spatie/laravel-permission": ">=3.0",
        "spatie/laravel-activitylog": ">=3.0",
        "laravel/ui": ">=2.0",
        "overtrue/laravel-options": "^2.0",
        "overtrue/laravel-uploader": "^1.2",
        "spatie/laravel-tags": "^2.6",
        "overtrue/pinyin": "~4.0",
        "spatie/laravel-query-builder": "^3.3",
        "tymon/jwt-auth": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shengfai\\LaravelAdmin\\": "src"
        },
        "files": [
            "src/helpers.php"
