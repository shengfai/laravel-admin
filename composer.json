    "description": "An administrative interface package for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "admin", "laravel admin"],
    "authors": [
        {
            "name": "shengfai",
            "email": "shengfai@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
